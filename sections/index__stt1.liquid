{%- assign html_content = section.settings.html_content -%}

{% style %}
  #shopify-section-{{ section.id }} {
    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
    padding-left: {{ section.settings.padding_left }}px;
    padding-right: {{ section.settings.padding_right }}px;
    {% if section.settings.width == 'wide' -%}
      width: 100%;
    {%- elsif section.settings.width == 'half' -%}
      width: 50%;
    {%- endif %}
  }
  {% render 'css-loop',
          css: section.settings.custom_css,
          id: section.id
  %}
.stt_featured {
    height: 26px;
}
{% endstyle %}

  <section class="product-home my-1 mb-xl-4 px-xl-4 splide mt-5" id="best-sellers" aria-label="Product Slider">
    <div itemtype="http://schema.org/ItemList">
      <div class="splide__track">
		<ul class="splide__list">
          {% for product in collections.st-tropica-best-sellers.products limit: 10 %}
          <li class="splide__slide px-2">
            {% render 'product-thumbnail-slide', product: product %}
          </li>
          {% endfor %}
          <li class="splide__slide px-2">
            <a href="/collections/st-tropica-collection">
              <img src="{{ 'shop_all_banner.png' | asset_img_url: '10x' }}" data-src="{{ 'shop_all_banner.png' | asset_url }}" class="lazyload transition--blur-up img-fluid-h pill-ingr img-full" alt="ST. TROPICA Hair Growth Vitamins"/>
